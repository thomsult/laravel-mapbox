{
    "name": "thomsult/laravel-mapbox",
    "description": "Laravel package pour les appels API Mapbox (geocoding, directions, etc.)",
    "type": "library",
    "keywords": [
        "laravel",
        "mapbox",
        "geocoding",
        "api",
        "maps",
        "directions",
        "search"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sultan Thomas",
            "email": "thomsult@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^10.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Thomsult\\LaravelMapbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomsult\\LaravelMapbox\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomsult\\LaravelMapbox\\Providers\\MapboxServiceProvider"
            ],
            "aliases": {
                "Mapbox": "Thomsult\\LaravelMapbox\\Facades\\Mapbox"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ]
    }
}